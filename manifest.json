{
  "manifest_version": 2,

  "name": "inaGiphy",
  "description": "quickly search Giphy from anywhere :)",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "index.html",
    "default_title": "Search GIFs from Giphy",
    "js": "content.js"
  },
  "permissions": [
    "activeTab",
    "http://api.giphy.com/"
  ]
}
